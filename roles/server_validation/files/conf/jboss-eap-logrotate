#Rotate all log files the same way
/opt/cox/log/jboss/*.log {
  compress
  missingok
  copytruncate
  nomail
  rotate 5
  daily
  dateext
  sharedscripts
  postrotate
     nice /etc/init.d/jboss cleanup 14 5 > /opt/cox/log/jboss/cleanup.log
  endscript
}

